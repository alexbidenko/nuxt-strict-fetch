{
  "name": "nuxt-strict-fetch",
  "version": "0.7.0",
  "description": "Nuxt Strict Fetch module",
  "author": "alexbidenko",
  "repository": "alexbidenko/nuxt-strict-fetch",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./types": "./dist/runtime/types.d.ts",
    "./common": {
      "types":  "./dist/runtime/utils/common/base.d.ts",
      "import": "./dist/runtime/utils/common/base.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "nuxi prepare",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.4",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.4",
    "@nuxt/test-utils": "^3.12.0",
    "@types/node": "^20.11.30",
    "changelogen": "^0.5.5",
    "eslint": "^9.7.0",
    "nuxt": "^3.12.4",
    "ofetch": "^1.3.4",
    "typescript": "^5.5.3",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.26"
  },
  "unbuild": {
    "failOnWarn": false
  }
}
